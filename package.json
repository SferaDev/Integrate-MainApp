{
  "name": "Integrate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest -u",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "react": "16.3.2",
    "react-dom": "^16.3.1",
    "react-native": "0.51.0",
    "react-native-dropdown-menu": "^2.0.0",
    "react-native-maps": "^0.20.0",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^1.5.9"
  },
  "devDependencies": {
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.2",
    "react-test-renderer": "16.3.2"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/__mocks__/**",
      "!**/node_modules/**",
      "!**/android/**",
      "!**/ios/**",
      "!**/coverage/**",
      "!**/index.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
